{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "My config",
            "program": "/workspaces/coreutils/target/pseudo-install/bin/pseudo",
            "args": [
                "/workspaces/coreutils/target/install-debug/usr/local/bin/ls",
                "--version",
            ],
            "environment": [
                {
                    "name": "PSEUDO_PREFIX",
                    "value": "/workspaces/coreutils/target/pseudo-install"
                },
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "/workspaces/coreutils/target/pseudo-install/lib64"
                }
            ],
            "cwd": "${workspaceFolder}",
            "setupCommands": [
                {
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                },
                {
                    "text": "set follow-fork-mode child",
                    "ignoreFailures": false
                },
                {
                    "text": "set detach-on-fork off",
                    "ignoreFailures": false
                }

            ],
            "stopAtEntry": false,
            "miDebuggerPath": "rust-gdb"
        }
    ]
}
